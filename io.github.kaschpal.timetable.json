{
    "app-id": "io.github.kaschpal.timetable",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "command": "timetable",
    "finish-args": [
        "--device=dri",
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-documents"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        "python3-modules.json",
        {
            "name": "timetable",
            "builddir": true,
            "buildsystem": "meson",
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/kaschpal/timetable.git",
                    "commit": "6a12bdb642fd5fab083df89c6fe1d73a8fe429e5"
                }
            ]
        }
    ]
}
